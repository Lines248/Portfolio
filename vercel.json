{
    "rewrites": [
      { "source": "/work", "destination": "/work.html" },
      { "source": "/about", "destination": "/about.html" },
    { "source": "/vending-machine", "destination": "/vending-machine.html" },
    { "source": "/accex", "destination": "/accex.html" },
    { "source": "/home", "destination": "/index.html" },
    { "source": "/", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/work.html", "destination": "/work", "permanent": true },
    { "source": "/about.html", "destination": "/about", "permanent": true },
    { "source": "/vending-machine.html", "destination": "/vending-machine", "permanent": true },
    { "source": "/accex.html", "destination": "/accex", "permanent": true },
    { "source": "/index.html", "destination": "/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' https://*.vercel-insights.com https://vercel.live; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self' data:; connect-src 'self' https://*.vercel-insights.com https://vercel.live; frame-ancestors 'none'; base-uri 'self'; form-action 'self';"
        }
      ]
    }
    ]
  }